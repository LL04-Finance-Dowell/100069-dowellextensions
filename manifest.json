{
  "name": "UX LivingLab",
  "description": "Extension to manage your UX LivingLab account. It comes with options like notification, login/logout, chat, logs, and setting.",
  "version": "1.111",
  "icons": {
     "16": "/images/16.png",
     "48": "/images/48.png",
     "128": "/images/128.png"
        },
  "author": "Dzhud_on_Github",
  "action": {
    "default_icon": "128.png",
    "default_popup": "popup.html",
    "default_title": "Manage Your UX LivingLab Account"
  },
  "content_scripts": [{
    "css": ["style.css"],
    "js": ["popup.js"],
    "all_frames": true,
     "run_at": "document_end",
     "matches": ["http://127.0.0.1.zxqpopw/*"]
  }],
  "permissions": ["tabs", "storage"],
  "manifest_version": 3

}

